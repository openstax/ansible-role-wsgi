     --capture-output \
     --access-logfile {{ openstax_wsgi_role.application_dir }}/logs/access.log \
     --error-logfile {{ openstax_wsgi_role.application_dir }}/logs/error.log \
     --bind unix:/tmp/gunicorn-{{ openstax_wsgi_role.application_name }}.sock"
